  <div class="  amber darken-2  black-text center-container" id="container1">
    <div id="forms">

      <!--TABS-->
      <div class="row ">
        <div class="col s12">
          <ul class="tabs tabs-fixed-width tab1  grey darken-4 orange-text">
            <li class="tab  "><a class="active cyan-text text-darken-2" href="#web-address">Web address</a></li>
            <li class="tab  "><a class=" cyan-text text-darken-2 "href="#web-code">HTML code</a></li>

          </ul>
        </div>
      </div>

      <!--TABS-->

      <div class="row ">


        <div id="web-address" class="col s12">
          <form   id="form-url" class=" row">
            <div class="input-field col s8 offset-s1">
              <div class="input-field inline">
                <input name="url" id="url" type="text" class="validate" value="" data-autosize-input="{ space": 40 }">
                <label for="url">URL</label>
              </div>
            </div>
            <div class=" col s2 ">
              <br>
              <button class="btn waves-effect waves-light cyan darken-3 " type="submit" name="action">Submit
              </button>

            </div>
          </form>



        </div>
      </div>
      <div id="web-code" class="col s12">

        <form  id="form-text" class=" row"">
        <div class="input-field col s7 offset-s1">
          <textarea  id="textarea-html" class="materialize-textarea"></textarea>
          <label for="textarea-html">Enter the HTML code to parse</label>
        </div>
        <div class="input-field col s2">
          <br>

          <button class="btn waves-effect waves-light cyan darken-3 " type="submit" name="action">Submit
          </button>
        </div>
        </form>

      </div>
    </div>



