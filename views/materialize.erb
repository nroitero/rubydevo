  <!--FIRST SECTION-->



  <!--FIRST SECTION-->


  <h5 class="white-text center-align">2 - Drag and drop methods</h5>

  <div class=" cyan darken-3 white-text center-align" id="container2">




    <form id="url-form">


      <!--<div class="chip">-->
      <!--Tag-->
      <!--<input class="hidden-input">-->
      <!--<i class="close material-icons">close</i>-->
      <!--</div>-->

      <!--TABS-->
      <div class="row ">
        <div class="col s12">
          <ul class="tabs tabs-fixed-width  grey darken-4 ">

            <%= @tabs %>

          </ul>
          <br>
        </div>
      </div>

      <div  class="row ">
        <%= @nokogiri %>


        <div class="container"><hr></div>


        <div class="input-field col s8 ">
          <ul id="drag" class="connectedSortable ">


            <li class="chip unsortable white">base</li>


            <li class='chip' data-type='1' data-method='css'>
              .css("<input type="text" id="autocomplete-input" class="autocomplete hidden-input cyan-text" autocomplete="false">")


              <i class="close material-icons">close</i>

            </li>


          </ul>

        </div>
        <div class="input-field col s4">
          <br>
          <a class="btn waves-effect waves-light orange" name="action" id="check">Check
          </a>
        </div>
      </div>
    </form>
